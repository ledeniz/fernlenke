<?xml version='1.0' encoding='utf-8'?>
<widget id="de.ozealis.fernlenke" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>fernlenke</name>
    <description>
        Mobile friendly stateless on/off switches for HTTP APIs
    </description>
    <author email="code@ledeniz.de" href="http://ledeniz.de">
        Deniz Erdogan
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <allow-intent href="market:*" />
    <icon src="res/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
